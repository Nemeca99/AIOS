{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Run UML Calculator",
            "type": "shell",
            "command": "python",
            "args": [
                "${workspaceFolder}/UML_Core/feature_demo.py"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "presentation": {
                "reveal": "always",
                "panel": "new"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": []
        },
        {
            "label": "Run Codex Web Calculator",
            "type": "shell",
            "command": "python",
            "args": [
                "${workspaceFolder:Algebra}/Calculator/codex_web_calculator.py"
            ],
            "options": {
                "cwd": "${workspaceFolder:Algebra}"
            },
            "presentation": {
                "reveal": "always",
                "panel": "new"
            },
            "group": "build",
            "problemMatcher": []
        },
        {
            "label": "Run Unified Calculator",
            "type": "shell",
            "command": "${workspaceFolder}/launch_unified_calculator.bat",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "presentation": {
                "reveal": "always",
                "panel": "new"
            },
            "group": "build",
            "problemMatcher": []
        }
    ]
}
